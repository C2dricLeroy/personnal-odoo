<odoo>

    <template id="homepage" name="Celer Homepage">
        <t t-call="celer_website.celer_layout">

          <t t-call="celer_website.celer_hero"/>
          <t t-call="celer_website.celer_service"/>
          <t t-call="celer_website.celer_cta"/>

        </t>
    </template>

    <record id="page_home" model="website.page">
        <field name="name">Home</field>
        <field name="url">/</field>
        <field name="view_id" ref="celer_website.homepage"/>
        <field name="is_published">True</field>
    </record>

</odoo>
